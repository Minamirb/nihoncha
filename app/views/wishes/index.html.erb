<h1>Listing wishes</h1>

<table>
  <tr>
    <th>Uri</th>
    <th>Name</th>
    <th>Price</th>
    <th>Image url</th>
    <th>Thumbneil url</th>
    <th>Bought</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wishes.each do |wish| %>
  <tr>
    <td><%= wish.uri %></td>
    <td><%= wish.name %></td>
    <td><%= wish.price %></td>
    <td><%= wish.image_url %></td>
    <td><%= wish.thumbneil_url %></td>
    <td><%= wish.bought %></td>
    <td><%= wish.user %></td>
    <td><%= link_to 'Show', wish %></td>
    <td><%= link_to 'Edit', edit_wish_path(wish) %></td>
    <td><%= link_to 'Destroy', wish, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Wish', new_wish_path %>
